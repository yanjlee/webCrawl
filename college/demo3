<script type="text/javascript"> 　　
		$(document).ready(function(){
	$.getJSON("http://souky.eol.cn/test/pro_rank.php?n=10&callback=?",function(data){
		//$.getJSON("http://souky6_ln.com/test/pro_rank.php?n=10&callback=?",function(data){
		$.each(data,function(i,val){
			var html = '<div class="row01"><div class="col_no">'+val.id+'</div><div class="col_major"><a target="_blank" href="http://souky.eol.cn/profession_search.php?searchword='+encodeURI(val.allproname)+'" title="'+val.allproname+'">'+val.allproname.substring(0,9)+'</a></div><div class="col_num">'+val.click+'</div><div class="col_qita"><a target="_blank"  href="http://souky.eol.cn/school_recommended.php?code='+val.pronamecode+'">开设院校</a></div></div>';
			$('#txy_1c').append(html);
		});
		var html2 = '<div class="row03">数据来源：<a href="http://souky.eol.cn">研招报名查询系统</a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<a href="http://souky.eol.cn/clicks.php">&gt;&gt;全部榜单</a></div>';
		$('#txy_1c').append(html2);
		});
		});
</script>

